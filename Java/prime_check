class prime_check
{
    public static void main(int n)
    {
        int i, flag=0;
        if(n==0||n==1)
        System.out.println("Not a prime number.");
        else
        for(i=2;i<=n/2;++i)
        {
            if(n%i==0)
            {
                flag=1;
                break;
            }
        }
        if(flag==1)
        System.out.println("Not a prime number.");
        else
        System.out.println("It is a prime number.");
    }
}
